<% if customer_signed_in? %>
  <%= form_with url: search_path,method: :get,local: true do |f| %>
    <%= f.text_field :content,placeholder:"Search" %>
    <%= f.select :model,options_for_select({"User"=>"customer","Post"=>"post","Hobby"=>"hobby"}) %>
    <%= f.select :method,options_for_select({"完全一致"=>"perfect","部分一致"=>"partial"}) %>
    <%= f.submit "Search",class:"btn btn-outline-info" %>
  <% end %>
<% end %>