<div class="conteiner">
  <div class="row-10 offset-1">
    <h2>Search result</h2>
    <div class="col m-4">
      <% if @model == 'customer'%>
        <h4><%= @content %></h4>
          <%= render 'customer/users/index',customers:@records %>
      <% elsif @model == 'post'%>
        <h4><%= @content %></h4>
          <%= render 'customer/posts/index',posts:@records %>
      <% else %>
        <h4><%= @content %></h4>
          <%= render 'customer/hobbies/hobby',hobbies:@records %>
      <% end %>
    </div>
  </div>
</div>