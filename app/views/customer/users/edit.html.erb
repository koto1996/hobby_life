<h1>UserEdit</h1>
 <%= form_with model:@customer,url: user_mypage_path do |f| %>
<table>
  <tr>
    <th>image</th>
    <td><%= f.file_field :profile_image,accept:"image/*"%></td>
  </tr>
  <tr>
    <th>name</th>
    <td><%= f.text_field :name %></td>
  </tr>
  <tr>
    <th></th>
    <td><%= f.submit 'Update',class:'btn btn-danger' %></td>
  </tr>
</table>
<% end %>