<!DOCTYPE html>
<html>
  <head>
    <title>HobbyLife</title>
    <meta name="viewport" content="width=device-width,initial-scale=1">
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>

    <%= stylesheet_pack_tag 'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_pack_tag 'application', 'data-turbolinks-track': 'reload' %>
  </head>

  <body>
    <div class="ml-5">
      <%=link_to "Log out",destroy_customer_session_path,method: :delete %>
    </div>
    <div class="ml-5">
      <%=link_to "Hobby",customer_hobbies_path %>
    </div>
    <div class="ml-5">
      <%=link_to "Post",customer_posts_path %>
    </div>
    <div class="ml-5">
      <%=link_to "Mypage",user_mypage_path %>
    </div>
    <%= yield %>
  </body>
</html>
